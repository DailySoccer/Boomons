<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.Unusual.Boomons"
      android:versionCode="1"
      android:versionName="1.0">

    <uses-sdk android:minSdkVersion="9" />
    
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE"/>
    
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
	<uses-feature android:name="android.hardware.sensor.gyroscope" />
    
    <application android:label="@string/app_name" android:icon="@drawable/app_icon" android:debuggable="false">
		 <activity android:name="com.unity3d.player.UnityPlayerActivity" android:launchMode="singleTask" android:label="@string/app_name">
			 <intent-filter>
				 <action android:name="android.intent.action.MAIN" />
				 <category android:name="android.intent.category.LAUNCHER" />
			 </intent-filter>
		 </activity>
		 <activity android:name="com.c4mprod.ezcodescanner.RootActivity" android:screenOrientation="behind"
                  android:label="@string/app_name">
        </activity>
        <activity android:name="com.c4mprod.ezcodescanner.camera.CameraActivity" android:screenOrientation="behind"/>
    </application>
</manifest> 
